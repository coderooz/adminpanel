## EXPIRES CACHING ##

RewriteCond %{REQUEST_FILENAME}% !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteCond $1 !^(config|core|css|js|fonts|robots\.txt)
RewriteRule ^(.*)$ $1.php [NC]

RewriteCond %{SCRIPT_FILENAME} !-f
RewriteCond %{SCRIPT_FILENAME} !-d
RewriteRule ^generator/(.*)$ generator.php?for=$1 [QSA]

FallbackResource /projects/adminpanel/index


###############################################

ErrorDocument 400 http://localhost/projects/adminpanel/400
ErrorDocument 401 http://localhost/projects/adminpanel/401
ErrorDocument 403 http://localhost/projects/adminpanel/403
ErrorDocument 404 http://localhost/projects/adminpanel/404
ErrorDocument 500 http://localhost/projects/adminpanel/500