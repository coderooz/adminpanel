RewriteEngine On

<ifModule mod_gip.c>
mod_gizp_on Yes
mod_gizp_dechunk Yes
mod_gizp_item_include file.(html?|txt|css|js|php|pl)$
mod_gizp_item_include handler ^cgi-script$
mod_gizp_item_include mime ^text/.*
mod_gizp_item_include mime ^application/x-javascript.*
mod_gizp_item_include mime ^image/.*
mod_gizp_item_include rspheader ^Content-Encoding:.*gzip.*
</ifModule>
<ifModule mod_headers.c>
	Header append Vary User-Agent env=!dont-vary
</ifModule>

